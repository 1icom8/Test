<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Lottery Show by ID</title>
</head>
<body>
    <table>
        <tr>
            <th>id</th>
            <th>Date</th>
            <th>Prize</th>
            <th>Numbers</th>
            <th>Payout</th>
        </tr>
        <% lottery.forEach(function(item) { %>
        <tr>
            <td><%= item.id %></td>
            <td><%= item.Date %></td>
            <td><%= item.Prize %></td>
            <td><%= item.Numbers %></td>
            <td><%= item.Payout %></td>
        </tr>
        <% }) %>
    </table>
</body>
</html>